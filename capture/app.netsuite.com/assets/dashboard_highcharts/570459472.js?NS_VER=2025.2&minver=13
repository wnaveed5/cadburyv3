KPIMeterDesign.prototype._adjustChartSettings=function(){var a=this.value;!this.isNegativeMeter&&this.value<this.min&&(a=this.min,this.chartValueOutOfBounds=!0);this.isNegativeMeter&&this.value>this.min&&(a=this.min,this.chartValueOutOfBounds=!0);return a};KPIMeterDesign.prototype.setValue=function(a){var b=this._adjustChartSettings();b=this._valueToAngle(b);this._drawChart(b);this._renderChartTextValue(a);this.changeThreshold(this.thresholdValue)};
KPIMeterDesign.prototype._renderMouseOverArea=function(){this.elements.mouseOverAreaArc=this.chart.renderer.arc(this.centerX,this.centerY,195*this.scale,.1,KPIMeterAngle.MIN,KPIMeterAngle.MAX).attr({fill:KPIMeterColors.TRANSPARENT,stroke:KPIMeterColors.TRANSPARENT,"stroke-width":0,zIndex:10}).add();return this.elements.mouseOverAreaArc};
KPIMeterDesign.prototype._prepareMouseOverToolTip=function(){var a=new StringBuffer;this.compareRangeNow&&a.append("<b>").append(this.compareRangeNow).append(":</b> ").append(this._formatNumberResult(this.value,Highcharts.numberFormat(Math.abs(this.value),0,".",","))).append("<br>");this.thresholdValue&&a.append("<b>").append(HardcodedStrings.THRESHOLD).append(":</b> ").append(this._formatNumberResult(this.thresholdValue,Highcharts.numberFormat(Math.abs(this.thresholdValue),0,".",",")));this.compareRangeLast&&
a.append("<br>").append("<b>").append(this.compareRangeLast).append(":</b> ").append(this._formatNumberResult(this.compareValueLast,Highcharts.numberFormat(Math.abs(this.compareValueLast),0,".",",")));this.elements.tooltipText=this.chart.renderer.text(a,this.centerX,this.centerY-80*this.scale).attr({align:"center",zIndex:9,opacity:0,"n-kpimeter-tooltip":"kmimeter-tooltip"}).css({color:this.meterColor.tooltipColor||KPIMeterColors.BLACK,fontSize:parseInt(16*this.scale)+"px",fontFamily:KPIMeterFonts,
width:this.chart.chartWidth+"px"}).add();if(a=this.elements.tooltipText.getBBox(!0)){const b=NS.UI.Util.isRedwood?6:0;this.tooltipBox=this.chart.renderer.rect(a.x-8*this.scale,a.y-8*this.scale,a.width+16*this.scale,a.height+16*this.scale,0).attr({fill:this.meterColor.tooltipFill,stroke:this.meterColor.color,"stroke-width":0,zIndex:8,opacity:0,rx:b,ry:b}).add()}};
KPIMeterDesign.prototype._handleKPIMeterMouseOver=function(){var a=this;if(!this._mouseOverArea)return!1;var b=function(){if(!this.elements.tooltipText||!this.tooltipBox)return!1;this.elements.tooltipText.animate({opacity:1},{duration:KPIMeterConf.tooltipAnimationTime});this.tooltipBox.animate({opacity:1},{duration:KPIMeterConf.tooltipAnimationTime})},c=function(d){if(!this.elements.tooltipText||!this.tooltipBox)return!1;this.elements.tooltipText.animate({opacity:0},{duration:KPIMeterConf.tooltipAnimationTime});
this.tooltipBox.animate({opacity:0},{duration:KPIMeterConf.tooltipAnimationTime,complete:d.bind(this)})};this._mouseOverArea.on("mouseover",function(){a.elements.chartValue&&a.elements.chartValue.animate({opacity:NS.UI.Util.isRedwood?1:0},{duration:KPIMeterConf.tooltipAnimationTime,complete:b.bind(a)})}).on("mouseout",function(){c.apply(a,[function(){this.elements.chartValue&&this.elements.chartValue.animate({opacity:1},{duration:KPIMeterConf.tooltipAnimationTime})}])})};
KPIMeterDesign.prototype._renderChartDimming=function(){this.elements.chartDimmingRect=this.chart.renderer.rect(0,0,this.chart.chartWidth,this.chart.chartHeight,0).attr({fill:"rgba(255, 255, 255, 0.8)",stroke:KPIMeterColors.WHITE,"stroke-width":0,zIndex:20}).add()};
KPIMeterDesign.prototype.dynamicNumProcessing=function(a){return{dataUnit:"",condensedNumText:1E9<=Math.abs(a)?Highcharts.numberFormat(a/1E9,1,".",",")+"B":1E6<=Math.abs(a)?Highcharts.numberFormat(a/1E6,1,".",",")+"M":1E4<=Math.abs(a)?Highcharts.numberFormat(a/1E3,1,".",",")+"K":Highcharts.numberFormat(a,0,".",",")+""}};
