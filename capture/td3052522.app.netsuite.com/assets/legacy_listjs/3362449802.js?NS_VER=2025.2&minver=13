function l_timenodes(a,b){var c=new Date,e=c;c=l_time(a,c);e=l_time(b,e);return c<e?-1:c==e?0:1}function l_time(a,b){/([0-9][0-9]?)(:[0-5][0-9])\s?([AaPp])?[Mm]?/.exec(a);a=parseInt(RegExp.$1);b=parseInt(RegExp.$2.substr(1));return 60*("pm"==(0==RegExp.$3.length||"A"==RegExp.$3.toUpperCase()?"am":"pm")?a%12+12:a%12)+b}
function l_num(a,b){a=a.replace(window.number_blacklist_regex,"");a=a.replace(window.decimalseparator,".");var c=window.negativeprefix.length,e=window.negativesuffix.length;if(0<c&&a.substring(0,c)==window.negativeprefix||0<e&&a.substring(a.length-e,a.length)==window.negativesuffix)a="-"+a.substring(c,a.length-e);if(isNaN(a)||""==a)a="INTEGER"==b?"0":"0.0";return"INTEGER"==b?parseInt(a):parseFloat(a)}
