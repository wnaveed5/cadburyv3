function l_sortnodes2(a,b,c){return-1!=c.indexOf("CURRENCY")||"INTEGER"==c||"FLOAT"==c||"POSFLOAT"==c||"PERCENT"==c||"NUMERICKEY"==c?l_numnodes(a,b,c):"PRIORITY"==c?l_prioritynodes(a,b):"DATE"==c||"MMYYDATE"==c||"DATETIME"==c?l_datenodes(a,b,"DATETIME"==c):"TIMEOFDAY"==c?l_timenodes(a,b):l_charnodes(a,b)}function l_charnodes(a,b){return l_isBlank(a)||l_isBlank(b)?(a=l_isBlank(a),b=l_isBlank(b),a&&!b?1:b&&!a?-1:0):a.toLowerCase().localeCompare(b.toLowerCase())}
